{
  "name": "rowantree.auth.typescript.sdk",
  "version": "0.1.0",
  "description": "The Rowan Tree Auth Service SDK [TypeScript]",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shapeandshare/rowantree.auth.typescript.sdk.git"
  },
  "author": "Joshua C. Burt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shapeandshare/rowantree.auth.typescript.sdk/issues"
  },
  "homepage": "https://github.com/shapeandshare/rowantree.auth.typescript.sdk#readme",
  "scripts": {
    "audit": "npm audit --registry=https://registry.npmjs.org/",
    "audit:fix": "npm audit fix --registry=https://registry.npmjs.org/",
    "build": "tsc",
    "clean": "rimraf dist .nyc_output coverage test-reports lint docs \"*.tgz\"",
    "clean:docs": "rimraf docs",
    "docs": "npm run clean:docs && typedoc",
    "lint": "eslint --ignore-path .gitignore --ext .ts .",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- -f json -o ./lint/lint_report.json",
    "pack": "npm run build && node build/pack.js",
    "prebuild": "npm run clean",
    "start": "tsc --watch",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "nyc --reporter=lcov --reporter=text mocha \"test/unit/**/*.test.ts\"",
    "test:integration": "mocha \"test/integration/**/*.test.ts\""
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": [
        "tscofig.json",
        "test/tsconfig.json"
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/eslint": "^8.4.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.17",
    "chai": "^4.3.6",
    "eslint": "^8.23.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.3"
  }
}
